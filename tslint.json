{
    "compilerOptions": {
      "target": "es5" ,
      "module": "commonjs",
      "lib": ["es2015"], 
      "jsx": "react" ,
      "strict": true,
      "esModuleInterop": true
    },
    "exclude": ["index.js", "android", "ios", "build", "node_modules", ".coverage", "native-assets", "jest"],
    "extends": [
          "tslint:latest",
          "tslint-config-prettier",
          "tslint-plugin-prettier",
          "tslint-react"
        ],
        "rules": {
            "prettier": [true, ".prettierrc"]
          }
  }